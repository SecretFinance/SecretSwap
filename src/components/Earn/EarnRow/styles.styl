.detail {

  font-size: 22px;
  font-family: "Work Sans Thin", BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
  font-weight: 600;
  line-height: 1.2;
  margin: 0;
  color: inherit;

  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  padding: 14px;

  >.image {
    width: 10%;
    background: transparent 0% 0% no-repeat;
    opacity: 1;
    margin-right: 15px;
  }

  >.symbol {
    width: 20%
  }

  >.apr {
    width: 25%
    paddingLeft: 10px;
  }

  >.tvl {
    float: none;
    width: 35%
    paddingLeft: 10px;

    >div>.earn_center_ele {
      justify-content: center !important;
      text-align: center !important;
    }
  }

  >.multiplier {
    float: right;
    width: 10%;
  }

  &.label {
    width: 300px
    display: flex;
    border: 1px solid #DEDEDE;
    box-sizing: border-box;
    border-radius: 4px;
    padding: 8.5px 8px;
    margin-top: 0.3em;
    margin-right: 1rem;
    font-family: 'Poppins', Arial, Helvetica, sans-serif;
    font-size: 22px;
    font-weight: 600;
  }

  &.light {
    background: white;
    color: #0D1017;

    strong {
      color: #0D1017;
    }
  }

  &.dark {
    background: transparent;
    color: white;

    strong {
      color: white;
      margin: 0 .5rem
    }
  }
}

@media only screen and (max-width: 1110px) {
  .detail {
    flex-flow: row wrap;
    >.symbol {
      margin-right: 10px;
    }
    >.apr {
      margin-left: 10px;
    }
    >.tvl {
      width: 50%;
      margin-top: 10px;
    }
    >.multiplier {
      margin-top: 10px;
    }
  }
}

