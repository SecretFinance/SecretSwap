
.accordion {
  width: 1168px;
  border: none;
  margin: 13px 0;
  position: relative;
  border-radius: 16px;
  .title {
  }

  @media only screen and (max-width: 1200px) {
    width: calc(100vw - 24px);
    min-width: auto;
  }

  @media only screen and (max-width: 425px) {
    width: calc(100vw - 24px);
    min-width: auto;
  }

  .responsive_row {
    display: grid;
    grid-auto-flow: column;
    justify-content: space-between;
    grid-template-columns: 100px 1fr 1fr 1fr 1fr;
    justify-self: center;
    @media only screen and (max-width: 768px) {
      grid-template-columns: 100px 1fr 1fr 1fr;
      pointer-events: none;
    }
    @media only screen and (max-width: 425px) {
      pointer-events: none;
      grid-template-columns: repeat(2, 1fr);
      grid-auto-flow: row;
      gap: 16px;
      padding: 26px 26px 26px 26px !important;
      div {
        padding-left: 6px;
        padding-bottom: 6px;
        &:nth-child(1){
          order:0;
        }
        &:nth-child(2){
          order:2;
        }
        &:nth-child(3){
          order:2;
        }
      }
    }
    .arrow {
      justify-self: end;
      @media only screen and (max-width: 768px) {
        display: none !important;
        // position: absolute;
        // right: 16px;
        // top: 0;
        // bottom: 0;
        // margin-bottom: auto !important;
        // margin-top: auto !important;
      }
    }
  }

  &.light {
    background: #ffffff;
    box-shadow: 0px 0px 5px 1px rgba(24, 39, 75, 0.12);
  }

  &.dark {
    background: #0E0E10;
    box-shadow: 0px 0px 10px 1px rgba(115, 115, 115, 0.12);
  }
}



